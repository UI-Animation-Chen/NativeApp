<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.czf.nativeapp.MainActivity">

    <Button
        android:id="@+id/send_msg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="send msg" />

    <Button
        android:id="@+id/start_new_mq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start new mq" />

    <Button
        android:id="@+id/quit_mq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="quit mq" />

    <Button
        android:id="@+id/send_msg_2_native"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="send msg 2 native" />

    <Button
        android:id="@+id/sleep_1_s"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="main thread sleep 1 s" />

    <ProgressBar
        android:layout_width="100dp"
        android:layout_height="100dp" />

</LinearLayout>
